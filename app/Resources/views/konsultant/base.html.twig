    <script>
    function show_stack_bottomright(type, text) {
    var opts = {
        title: "Info",
        text: text,
        styling: 'bootstrap3',
        addclass: "stack-bottomright"
       
    };
    switch (type) {
    case 'error':
        opts.title = "Info";
        opts.text = text;
        opts.type = "error";
        break;
    case 'info':
        opts.title = "Info";
        opts.text = text;
        opts.type = "info";
        break;
    case 'success':
        opts.title = "Info";
        opts.text = text;
        opts.type = "success";
        break;
    }
    new PNotify(opts);
    } 
    </script>
